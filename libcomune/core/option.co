using core::libc::abort;

enum Option<type T> {
	Some { 
		T value;

		new(new* self, T value) {
			*self = new Option<T>::Some { value };
		}
	},

	None,
}

impl<type T> Option<T> {
	T unwrap(self) {
		match self {
			Option<T>::Some some => {
				return some.value
			}

			Option<T>::None _ => {
				abort()
			}
		}
	}
}