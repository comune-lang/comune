// comune core library - traits module
//
// this module contains traits that are fundamental to the type system.

@lang(Copy)
trait Copy {
	Self copy(&self);
}

@lang(Move)
trait Move {
	Self move(mut& self);
}

@lang(Drop)
trait Drop {
	void drop(Self mut& self);
}

@lang(Sized)
trait Sized {}

@lang(Send)
trait Send {}

@lang(Sync)
trait Sync {}