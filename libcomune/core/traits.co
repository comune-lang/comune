// comune core library - traits module
//
// this module contains traits that are fundamental to the type system.

@lang(Copy)
trait Copy {}

@lang(Clone)
trait Clone {
	Self clone(&self);
}

@lang(Drop)
trait Drop {
	void drop(mut& self);
}

@lang(Sized)
trait Sized {}

@lang(Send)
trait Send {}

@lang(Sync)
trait Sync {}