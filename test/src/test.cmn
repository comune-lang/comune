import libc;
import module_test;

using module_test::foo;
using my_cool_alias = libc::printf;

namespace cool {
	u64 fib(u64 n) {
		u64 a = 0;
		u64 b = 1;
		u64 c = 0;
		for (u64 i = 1; i < n; i += 1u64) {
			c = a + b;
			a = b;
			b = c;
			my_cool_alias("loop!\n");
		}
		return c;
	}

	struct TestStruct {
		int a;
		int b;
		int* c;
		TestStructB* d;
	}

	struct TestStructB {
		TestStruct a;
	}
}

int main() {
	cool::TestStruct a;
	a.a = 23;
	return a.a + cool::fib(10) as i32 + foo();
}