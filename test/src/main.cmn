@no_mangle 
void printf(char* fmt);

@layout(packed)
struct Test {
	int a;
	int b;
}

impl Test {
	int sum(Test self) {
		printf("This is a method call!\n\0");
		return self.a + self.b;
	}
}

void no_return() {
	printf("This void fn doesn't have a return statement!\n\0");
}

int main() {
	printf("This is main()!\n\0");
	
	Test test = Test { 
		a: 60, 
		b: 0 
	};
	
	while (test.b < 9) {
		printf("This is a while loop!\n\0");
		test.b += 1;
	}

	for (int i = 0; i < 10; i += 1) {
		printf("For loop test!\n\0");
	}
	
	return test.sum();
}
