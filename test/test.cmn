@no_mangle
int printf(char* fmt);

namespace cool {
	u64 fib(u64 n) {
		u64 a = 0; // Literals can be coerced to any numeric type
		u64 b = 1;
		u64 c = 0;
		for (u64 i = 1; i < n; i = i + 1u64) {
			c = a + b;
			a = b;
			b = c;
			printf("loop!\n");
		}
		return c;
	}

	struct Test {
		Subtest a;	
	}

	struct Subtest {
		Test b;
	}
} 

int main() {
	cool::Test test;
	cool::Test* testptr = &test;
	(*testptr).a.b = testptr;
	return cool::fib(10) as i32;
}
